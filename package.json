{
  "name": "more-tools",
  "version": "1.0.0-alpha.0",
  "type": "module",
  "scripts": {
    "build:web": "tsc && vite build -c vite.config.web.ts",
    "build:native": "tsc && vite build -c vite.config.native.ts"
  },
  "dependencies": {
    "@babel/runtime": "^7.22.5"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^3.0.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-native-vector-icons": "^6.4.13",
    "@vitejs/plugin-react": "^4.0.0",
    "clsx": "^1.2.1",
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-native-vector-icons": "^9.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.0",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "main": "./dist/main.web.umd.cjs",
  "module": "./dist/main.web.js",
  "react-native": "./dist/native/main.native.js",
  "exports": {
    "for-web": {
      "require": {
        "default": "./dist/web/main.web.cjs",
        "types": "./dist/web/main.web.d.ts"
      },
      "import": {
        "default": "./dist/web/main.web.js",
        "types": "./dist/web/main.web.d.ts"
      }
    },
    "for-uno": {
      "require": {
        "default": "./dist/web/main.uno.cjs",
        "types": "./dist/web/main.uno.d.ts"
      },
      "import": {
        "default": "./dist/web/main.uno.js",
        "types": "./dist/web/main.uno.d.ts"
      }
    },
    "for-native": {
      "require": {
        "default": "./dist/native/main.native.umd.cjs",
        "types": "./dist/native/main.native.d.ts"
      },
      "import": {
        "default": "./dist/native/main.native.js",
        "types": "./dist/native/main.native.d.ts"
      }
    }
  },
  "files": [
    "lib"
  ],
  "workspaces": [
    "examples/*"
  ],
  "packageManager": "yarn@3.5.1"
}
