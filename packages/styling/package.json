{
  "name": "@earthling-ui/styling",
  "version": "1.0.0-alpha.2",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sFrady20/earthling-ui",
    "directory": "packages/primitives"
  },
  "bugs": {
    "url": "https://github.com/sFrady20/earthling-ui/issues"
  },
  "author": {
    "email": "sFrady20@gmail.com",
    "name": "Steven Frady",
    "url": "https://stevenfrady.com"
  },
  "scripts": {
    "build": "yarn build:web; yarn build:native;",
    "build:web": "vite build -c vite.config.web.ts",
    "build:native": "vite build -c vite.config.native.ts"
  },
  "devDependencies": {
    "@earthling-ui/config": "workspace:^",
    "@types/css": "^0.0.37",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@vitejs/plugin-react": "^4.2.0",
    "css-to-react-native": "^3.2.0",
    "csstype": "^3.1.2",
    "react": "^18.2.0",
    "react-native": "^0.72.7",
    "typescript": "^5.3.2",
    "vite": "^5.0.3",
    "vite-plugin-dts": "^3.6.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "main": "./dist/web/index.umd.cjs",
  "module": "./dist/web/index.js",
  "react-native": "./dist/native/index.js",
  "imports": {
    "#*": {
      "react-native": "./src/*/index.native.tsx",
      "import": "./src/*/index.web.tsx",
      "default": "./src/*/index.tsx"
    }
  },
  "exports": {
    ".": {
      "react-native": {
        "types": "./dist/native/index.d.ts",
        "default": "./dist/native/index.js"
      },
      "import": {
        "types": "./dist/web/index.d.ts",
        "default": "./dist/web/index.js"
      },
      "types": "./dist/web/index.d.ts",
      "default": "./dist/web/index.umd.cjs"
    },
    "./types": "./lib/types.d.ts"
  },
  "dependencies": {
    "@unocss/core": "^0.57.7",
    "@unocss/preset-uno": "^0.57.7",
    "css": "^3.0.0"
  }
}
