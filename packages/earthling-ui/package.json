{
  "name": "earthling-ui",
  "version": "1.1.0-alpha.22",
  "homepage": "https://ui.earthling.dev",
  "bugs": "https://github.com/earthling-dev/earthling-ui/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earthling-dev/earthling-ui.git",
    "directory": "packages/earthling-ui"
  },
  "license": "MIT",
  "author": {
    "name": "Steven Frady",
    "email": "sFrady20@gmail.com",
    "url": "https://stevenfrady.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "style": "./src/index.css"
    },
    "./theme/*": {
      "style": "./src/themes/*.css"
    },
    "./utils/*": {
      "default": "./dist/utils/*.js"
    },
    "./*": {
      "default": "./dist/components/*/index.js",
      "types": "./dist/components/*/index.tsx"
    }
  },
  "style": "./src/index.css",
  "bin": {
    "earthling-ui": "src/cli.ts"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "bun run scripts/build.ts",
    "dev": "bun run build && chokidar \"src/**/*.{ts,tsx}\" -c \"bun run scripts/build.ts\""
  },
  "dependencies": {
    "@clack/prompts": "^0.10.0",
    "@iconify/tailwind4": "^1.0.1",
    "commander": "^13.1.0",
    "copy-paste": "^1.5.3",
    "find-parent-dir": "^0.3.1",
    "isomorphic-git": "^1.29.0"
  },
  "devDependencies": {
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-popover": "^1.1.5",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@types/bun": "latest",
    "@types/copy-paste": "^1.1.33",
    "@types/find-parent-dir": "^0.3.3",
    "@types/node": "^22.12.0",
    "bun-plugin-dts": "^0.3.0",
    "chokidar-cli": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "react-aria-components": "^1.6.0",
    "tailwind-merge": "^2.6.0",
    "type-fest": "^4.34.1",
    "vaul": "^1.1.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
