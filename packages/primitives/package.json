{
  "name": "@earthling-ui/primitives",
  "version": "1.0.0-alpha.0",
  "type": "module",
  "scripts": {
    "build": "yarn build:web; yarn build:native;",
    "build:web": "vite build -c vite.config.web.ts",
    "build:native": "vite build -c vite.config.native.ts"
  },
  "devDependencies": {
    "@earthling-ui/config": "workspace:^",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@vitejs/plugin-react": "^4.0.1",
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "main": "./dist/main.web.cjs",
  "module": "./dist/main.web.js",
  "react-native": "./dist/main.native.js",
  "exports": {
    "./*": {
      "require": {
        "default": "./dist/web/*.cjs",
        "types": "./dist/web/*/web.d.ts"
      },
      "import": {
        "default": "./dist/web/*.js",
        "types": "./dist/web/*/web.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ]
}
