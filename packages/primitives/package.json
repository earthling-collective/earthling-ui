{
  "name": "@earthling-ui/primitives",
  "version": "1.0.0-alpha.0",
  "type": "module",
  "scripts": {
    "build": "vite build"
  },
  "devDependencies": {
    "@earthling-ui/config": "workspace:^",
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.13",
    "@vitejs/plugin-react": "^4.0.1",
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-native": "^0.72.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "files": [
    "dist"
  ],
  "main": "./dist/web.cjs",
  "module": "./dist/web.js",
  "react-native": "./dist/native.js",
  "exports": {
    "./web": {
      "require": {
        "default": "./dist/web.cjs",
        "types": "./dist/web/index.d.ts"
      },
      "import": {
        "default": "./dist/web.js",
        "types": "./dist/web/index.d.ts"
      }
    },
    "./native": {
      "require": {
        "default": "./dist/native.cjs",
        "types": "./dist/native/index.d.ts"
      },
      "import": {
        "default": "./dist/native.js",
        "types": "./dist/native/index.d.ts"
      }
    },
    "./*": {
      "require": {
        "default": "./dist/*.cjs",
        "types": "./dist/*/index.d.ts"
      },
      "import": {
        "default": "./dist/*.js",
        "types": "./dist/*/index.d.ts"
      }
    }
  }
}
