{
  "name": "@earthling-ui/primitives",
  "version": "1.0.0-alpha.0",
  "type": "module",
  "scripts": {
    "build:web": "vite build -c vite.config.web.ts",
    "build:native": "vite build -c vite.config.native.ts"
  },
  "devDependencies": {
    "@tsconfig/react-native": "^3.0.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-native-vector-icons": "^6.4.13",
    "@unocss/vite": "^0.53.1",
    "@vitejs/plugin-react": "^4.0.0",
    "clsx": "^1.2.1",
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-native-vector-icons": "^9.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.0",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    }
  },
  "main": "./dist/main.web.cjs",
  "module": "./dist/main.web.js",
  "react-native": "./src/main.native.tsx",
  "exports": {
    "./alert": {
      "require": {
        "default": "./dist/web/alert.cjs",
        "types": "./dist/web/alert/web.d.ts"
      },
      "import": {
        "default": "./dist/web/alert.js",
        "types": "./dist/web/alert/web.d.ts"
      }
    },
    "./button": {
      "require": {
        "default": "./dist/web/button.cjs",
        "types": "./dist/web/button/web.d.ts"
      },
      "import": {
        "default": "./dist/web/button.js",
        "types": "./dist/web/button/web.d.ts"
      }
    }
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json",
    "tsconfig.node.json"
  ],
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "packageManager": "yarn@3.5.1"
}
