{
  "name": "@earthling-ui/components",
  "version": "1.0.0-alpha.0",
  "type": "module",
  "scripts": {
    "build": "yarn build:web; yarn build:native;",
    "build:web": "vite build -c vite.config.web.ts",
    "build:native": "vite build -c vite.config.native.ts"
  },
  "devDependencies": {
    "@earthling-ui/config": "workspace:^",
    "@earthling-ui/primitives": "workspace:^",
    "@earthling-ui/stylist": "workspace:^",
    "@tsconfig/react-native": "^3.0.2",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-native-vector-icons": "^6.4.13",
    "@unocss/vite": "^0.53.1",
    "@vitejs/plugin-react": "^4.0.0",
    "clsx": "^1.2.1",
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-native-vector-icons": "^9.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.0",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*"
  },
  "peerDependenciesMeta": {
    "react-dom": {
      "optional": true
    },
    "react-native": {
      "optional": true
    }
  },
  "main": "./dist/main.web.cjs",
  "module": "./dist/main.web.js",
  "react-native": "./dist/main.native.js",
  "exports": {
    "./*": {
      "require": {
        "default": "./dist/web/*.umd.cjs",
        "types": "./dist/web/main.web.d.ts"
      },
      "import": {
        "default": "./dist/web/main.web.js",
        "types": "./dist/web/main.web.d.ts"
      }
    }
  },
  "files": [
    "dist"
  ]
}
